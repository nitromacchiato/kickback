generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["nativeTypes"]
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:baseball1@localhost:3306/kickback"
}

model Account {
  id                 Int       @id @default(autoincrement()) @db.Int
  compoundId         String    @unique @map("compound_id") @db.VarChar(191)
  userId             Int       @map("user_id") @db.Int
  providerType       String    @map("provider_type") @db.VarChar(191)
  providerId         String    @map("provider_id") @db.VarChar(191)
  providerAccountId  String    @map("provider_account_id") @db.VarChar(191)
  refreshToken       String?   @map("refresh_token") @db.VarChar(191)
  accessToken        String?   @map("access_token") @db.VarChar(500)
  accessTokenExpires DateTime? @map("access_token_expires") @db.Datetime(3)
  createdAt          DateTime  @default(now()) @map("created_at") @db.Datetime(3)
  updatedAt          DateTime  @default(now()) @map("updated_at") @db.Datetime(3)

  @@index([providerAccountId], name: "providerAccountId")
  @@index([providerId], name: "providerId")
  @@index([userId], name: "userId")
  @@map("accounts")
}

model Session {
  id           Int      @id @default(autoincrement()) @db.Int
  userId       Int      @map("user_id") @db.Int
  expires      DateTime @db.Datetime(3)
  sessionToken String   @unique @map("session_token") @db.VarChar(191)
  accessToken  String   @unique @map("access_token") @db.VarChar(191)
  createdAt    DateTime @default(now()) @map("created_at") @db.Datetime(3)
  updatedAt    DateTime @default(now()) @map("updated_at") @db.Datetime(3)

  @@map("sessions")
}

model User {
  id            Int       @id @default(autoincrement()) @db.Int
  name          String?   @db.VarChar(191)
  email         String?   @unique @db.VarChar(191)
  emailVerified DateTime? @map("email_verified") @db.Datetime(3)
  image         String?   @db.VarChar(191)
  createdAt     DateTime  @default(now()) @map("created_at") @db.Datetime(3)
  updatedAt     DateTime  @default(now()) @map("updated_at") @db.Datetime(3)

  @@map("users")
}

model VerificationRequest {
  id         Int      @id @default(autoincrement()) @db.Int
  identifier String   @db.VarChar(191)
  token      String   @unique @db.VarChar(191)
  expires    DateTime @db.Datetime(3)
  createdAt  DateTime @default(now()) @map("created_at") @db.Datetime(3)
  updatedAt  DateTime @default(now()) @map("updated_at") @db.Datetime(3)

  @@map("verification_requests")
}

model schools {
  school_id Int     @id @default(autoincrement()) @db.Int
  name      String  @unique @db.VarChar(191)
  email     String  @unique @db.VarChar(191)
  alt_email String? @db.VarChar(191)
  nickname  String? @db.VarChar(191)
}

model SchoolEmailVerification {
  id                  Int    @id @default(autoincrement()) @db.Int
  user_name           String @unique @db.VarChar(191)
  school_email        String @unique @db.VarChar(191)
  random_school_index String @unique @db.VarChar(191)

  @@map("schoolemailverificaiton")
}
